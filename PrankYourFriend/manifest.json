{
    "manifest_version":2,
    "name": "Prank Your Friend Extension",
    "description": "Use this extension to prank your friend ;)",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "browser_action" : {
        "default_popup" : "popup.html",
        "default_icon": "/images/logo3.png"
    },
    "icons" : {
        "128": "/images/logo3.png"
    }
}
