{
    "name": "Birthday Reminder",
    "description": "Never miss your birthday again!",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "activeTab",
        "background"
    ]
}